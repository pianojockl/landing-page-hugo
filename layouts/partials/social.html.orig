{{ range .Site.Params.social }}
	<li>
		{{ if eq .targetblank "false" }}
		<a href="{{ .url }}" class="btn btn-default btn-lg">
		{{ else }}
		<a href="{{ .url }}" target="_blank" class="btn btn-default btn-lg">
		{{ end }}
			<i class="fa fa-{{ .icon }} fa-fw"></i>
			<span class="network-name">{{ .title }}</span>
		</a>
	</li>
{{ end }}
